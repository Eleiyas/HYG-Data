  {
    "textid": "KMTextInfo_70006_169_fakeName",
    "content": "多多",
    "info": ""
  },
  {
    "textid": "KMTextInfo_70006_169_content",
    "content": "今天要播报的内容……\n好像，只有这些？！",
    "info": ""
